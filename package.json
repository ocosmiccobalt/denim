{
  "name": "denim",
  "version": "1.0.0",
  "private": true,
  "description": "Denim online store",
  "type": "module",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\"",
    "eslint": "eslint source/js/",
    "test": "npm run editorconfig && npm run stylelint && npm run eslint",
    "build": "gulp buildProd",
    "start": "gulp"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@htmlacademy/editorconfig-cli": "^1.0.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "browser-sync": "^2.28.3",
    "del": "^7.0.0",
    "eslint": "^8.35.0",
    "eslint-config-htmlacademy": "^9.0.0",
    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-posthtml": "^3.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-svgstore": "^9.0.0",
    "postcss": "^8.4.21",
    "postcss-scss": "^4.0.6",
    "posthtml-include": "^1.7.4",
    "sass": "^1.58.3",
    "stylelint": "^14.16.1",
    "stylelint-config-htmlacademy": "^1.1.4",
    "webpack": "^5.75.0",
    "webpack-stream": "^7.0.0"
  },
  "dependencies": {
    "core-js": "^3.29.0"
  }
}
